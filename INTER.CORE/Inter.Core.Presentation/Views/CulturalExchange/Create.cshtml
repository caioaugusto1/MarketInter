@model Inter.Core.App.ViewModel.CulturalExchangeViewModel

@await Html.PartialAsync("~/Views/Shared/_partial/_topMenu.cshtml")

<div class="container">

    <h1 class="h3 mb-2 text-gray-800">Cultural Exchange - NEW</h1>

    <table class="table table-striped">
        <tbody>
            <tr>
                <td colspan="1">
                    <form id="form-create-culturalExchange" asp-action="OnCreate" class="well form-horizontal">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                        @await Html.PartialAsync("~/Views/CulturalExchange/_partial/_form_create_edit.cshtml", Model)

                        <input hidden asp-for="EnviromentId" value="@ViewBag.EnvironmentId" />

                    </form>
                </td>
            </tr>
        </tbody>
    </table>
</div>


@section Scripts {

    <script src="~/js/Util/Util.js"></script>
    <script src="~/js/CulturalExchange/culturalExchange.js"></script>

    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}