@model ReceivePaymentCulturalExchangeViewModel

<div class="card-body">

    <dl class="row">
        <dt class="col-sm-6">
            Customer Id
        </dt>
        <dd class="col-sm-6">
            @Html.DisplayFor(model => model.CulturalExchange.StudentViewModel.CustomerId)
        </dd>
        <dt class="col-sm-6">
            Name
        </dt>
        <dt class="col-sm-6">
            @Html.DisplayFor(model => model.CulturalExchange.StudentViewModel.FullName)
        </dt>
        <dt class="col-sm-6">
            Date of Birthday
        </dt>
        <dd class="col-sm-6">
            @Html.DisplayFor(model => model.CulturalExchange.StudentViewModel.DateOfBirthday)
        </dd>
        <dt class="col-sm-6">
            College
        </dt>
        <dd class="col-sm-6">
            @Html.DisplayFor(model => model.CulturalExchange.CollegeViewModel.Name)
        </dd>
        <dt class="col-sm-6">
            Period
        </dt>
        <dd class="col-sm-6">
            @Html.DisplayFor(model => model.CulturalExchange.CollegeTimeViewModel.Period)
        </dd>
        <dt class="col-sm-6">
            Start Time
        </dt>
        <dd class="col-sm-6">
            @Html.DisplayFor(model => model.CulturalExchange.CollegeTimeViewModel.StartTime)
        </dd>
        <dt class="col-sm-6">
            Finish Time
        </dt>
        <dd class="col-sm-6">
            @Html.DisplayFor(model => model.CulturalExchange.CollegeTimeViewModel.FinishTime)
        </dd>
        <dt class="col-sm-6">
            Total Value
        </dt>
        <dd class="col-sm-6">
            @Html.DisplayFor(model => model.CulturalExchange.TotalValue)
        </dd>
    </dl>
</div>