@model Inter.Core.App.ViewModel.CollegeViewModel

@await Html.PartialAsync("~/Views/Shared/_partial/_topMenu.cshtml")

@await Html.PartialAsync("~/Views/College/_partial/_collegeDetails.cshtml")

<div class="modal fade" id="modalinsertTimeCollege" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Time College</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form id="form-create-collegeTime">

                    @await Html.PartialAsync("~/Views/College/_partial/_modalPutTimeCollege.cshtml")

                    <input type="hidden" name="CollegeId" value="@Model.Id" />
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button type="button" onclick="college.saveCollegeTime();" class="btn btn-primary">Save</button>
            </div>
        </div>
    </div>
</div>


@section Scripts {

    <script src="~/js/Util/Util.js"></script>
    <script src="~/js/College/college.js"></script>

    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
